[
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Hotel",
        "fieldname": "custom_hotel_tab",
        "fieldtype": "Tab Break",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Hotel Property",
        "fieldname": "custom_hotel_property",
        "fieldtype": "Link",
        "options": "CRM Hotel Property",
        "insert_after": "custom_hotel_tab",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Extra Bed Rate",
        "fieldname": "custom_extra_bed_rate",
        "fieldtype": "Float",
        "fetch_from": "custom_hotel_property.extra_bed_rate",
        "read_only": 1,
        "precision": "2",
        "insert_after": "custom_hotel_property",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Check In",
        "fieldname": "custom_check_in",
        "fieldtype": "Date",
        "default": "Today",
        "insert_after": "custom_extra_bed_rate",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Check Out",
        "fieldname": "custom_check_out",
        "fieldtype": "Date",
        "insert_after": "custom_check_in",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Nights",
        "fieldname": "custom_no_of_nights",
        "fieldtype": "Int",
        "read_only": 1,
        "insert_after": "custom_check_out",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Guest",
        "fieldname": "custom_guest",
        "fieldtype": "Table",
        "options": "CRM Guest Details",
        "insert_after": "custom_no_of_nights",
        "depends_on": "eval: !!doc.custom_hotel_property",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Tax Amount",
        "fieldname": "custom_tax_amount",
        "fieldtype": "Float",
        "read_only": 1,
        "precision": "2",
        "description": "(Fixed GST 18%)",
        "insert_after": "custom_guest",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Rates",
        "fieldname": "custom_rates",
        "fieldtype": "Float",
        "read_only": 1,
        "precision": "2",
        "description": "Taxable Amount + Tax amount",
        "insert_after": "custom_tax_amount",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "City",
        "fieldname": "custom_city",
        "fieldtype": "Data",
        "insert_after": "custom_rates",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "State",
        "fieldname": "custom_state",
        "fieldtype": "Data",
        "insert_after": "custom_city",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Country",
        "fieldname": "custom_country",
        "fieldtype": "Data",
        "insert_after": "custom_state",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Pincode",
        "fieldname": "custom_pincode",
        "fieldtype": "Data",
        "insert_after": "custom_country",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Remarks",
        "fieldname": "custom_remarks",
        "fieldtype": "Text",
        "insert_after": "custom_pincode",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "User Type",
        "fieldname": "custom_user_type",
        "fieldtype": "Select",
        "options": "Individual\nCorporate\nAgent",
        "default": "Individual",
        "insert_after": "details",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Sub Total",
        "fieldname": "custom_sub_total",
        "fieldtype": "Float",
        "read_only": 1,
        "precision": "2",
        "description": "Sum of Line Total",
        "insert_after": "custom_user_type",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Discount(%)",
        "fieldname": "custom_discount_percent",
        "fieldtype": "Float",
        "precision": "2",
        "insert_after": "custom_sub_total",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Discount Amount",
        "fieldname": "custom_discount_amount",
        "fieldtype": "Float",
        "read_only": 1,
        "precision": "2",
        "insert_after": "custom_discount_percent",
        "module": "Praveg",
        "is_system_generated": 1
    },
    {
        "doctype": "Custom Field",
        "dt": "CRM Lead",
        "label": "Taxable Amount",
        "fieldname": "custom_taxable_amount",
        "fieldtype": "Float",
        "read_only": 1,
        "precision": "2",
        "insert_after": "custom_discount_amount",
        "module": "Praveg",
        "is_system_generated": 1
    }
]